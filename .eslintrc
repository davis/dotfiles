{
  "parser": "babel-eslint",
  "parserOptions": {
    ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "plugins": ["react"],
  "rules": {
  	"no-console": 0,
    "semi": [2, "always"],
    "react/react-in-jsx-scope": 0
  }
}
